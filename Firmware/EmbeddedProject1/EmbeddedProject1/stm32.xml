  <BspID>com.sysprogs.arm.stm32</BspID>
  <McuID>STM32F405VG</McuID>
  <MCUDefinitionFile>devices\stm32f405xx.xml</MCUDefinitionFile>
  <MCUProperties>
    <Entries>
      <KeyValue>
        <Key>com.sysprogs.bspoptions.primary_memory</Key>
